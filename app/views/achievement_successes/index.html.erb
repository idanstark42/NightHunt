<p id="notice"><%= notice %></p>

<h1>Listing achievement Successes</h1>

<table>
  <thead>
    <tr>
      <th>achievement</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @achievement_successes.each do |achievement_success| %>
      <tr>
        <td><%= achievement_success.achievement %></td>
        <td><%= achievement_success.team %></td>
        <td><%= link_to 'Show', achievement_success %></td>
        <td><%= link_to 'Edit', edit_achievement_success_path(achievement_success) %></td>
        <td><%= link_to 'Destroy', achievement_success, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Achievement success', new_achievement_success_path %>
